#raw credit hours for everyone
raw_credithrs <- merge_r %>%
  group_by(treatment) %>%
  summarise_at(vars(Sp2024TotalEarnHrs), list(name = mean))

#raw credit hours only for those still in school
subset_r <- subset(merge_r, Spring2024withdrew.dummy == 0 &
                     Spring2024Graduation.dummy == 0)

raw_credithrs <- subset_r %>%
  group_by(treatment) %>%
  summarise_at(vars(Sp2024TotalEarnHrs), list(name = mean))


#logistic regression for withdrawals
#Spring 2021 withdrawals
WDSpring21 <- glm(Spring21withdrew.Dummy~
                    treatment+
                    Sex+
                    Ethnic_Black+
                    Ethnic_Hispanic+
                    Class_SO+
                    College_ARTS+
                    College_BUS+
                    College_EDUC+
                    College_NHHS+
                    College_TECH,
                  data = merge_r,
                  family = "binomial")

summary(WDSpring21)

options(scipen=999) 
exp(coef(WDSpring21))

#Fall 2021 withdrawals 
WDFall21 <- glm(Fall21withdrew.Dummy~
                  treatment+
                  Sex+
                  Ethnic_Black+
                  Ethnic_Hispanic+
                  Class_SO+
                  College_ARTS+
                  College_BUS+
                  College_EDUC+
                  College_NHHS+
                  College_TECH,
                data = merge_r,
                family = "binomial")

summary(WDFall21)

options(scipen=999) 
exp(coef(WDFall21))

