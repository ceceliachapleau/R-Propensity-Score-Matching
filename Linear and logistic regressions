#replicate models for credit hours
creditsS21 <- lm(SpringTotalEarnHrs~
                   treatment+
                   TotalEarnHrs+
                   Sex+
                   Ethnic_Black+
                   Ethnic_Hispanic+
                   Class_SO+
                   College_ARTS+
                   College_BUS+
                   College_EDUC+
                   College_NHHS+
                   College_TECH,
                 data = merge_r)

summary(creditsS21)


#Fall 2021 treatment impact on Credit Hours
