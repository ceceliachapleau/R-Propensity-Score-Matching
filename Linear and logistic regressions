raw_grad_r <- merge_r %>%
  group_by(treatment) %>%
  summarise_at(vars(fall22_grad, sp23_grad, Fall2023Graduation.dummy,
                    Spring2024Graduation.dummy), list(name = sum)) 

#Testing impact on GPA
